class Animal:
    def make_sound(self):
        return "Some generic sound"

class Dog(Animal):
    def make_sound(self):
        return "Woof!"

class Cat(Animal):
    def make_sound(self):
        return "Meow!"

# Uso del polimorfismo
animals = [Dog(), Cat(), Animal()]

for animal in animals:
    print(animal.make_sound())
    # Salida:
    # Woof!
    # Meow!
    # Some generic sound
